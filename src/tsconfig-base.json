{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES2019",
        "lib": [
            "ES2019"
        ],
        "moduleResolution": "node",
        "sourceMap": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "noImplicitThis": false,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "strict": true
    }
}
